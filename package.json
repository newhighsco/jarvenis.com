{
  "name": "@newhighsco/jarvenis.com",
  "description": "Jarvenis website",
  "version": "1.2.1",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Jarvenis"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/jarvenis.com"
  },
  "homepage": "https://jarvenis.com",
  "bugs": {
    "url": "https://github.com/newhighsco/jarvenis.com/issues"
  },
  "scripts": {
    "start": "next dev",
    "build": "npm-run-all build:next",
    "build:next": "next build && next export --outdir build",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "4.0.0",
    "@newhighsco/next-plugin-css-options": "1.0.1",
    "@newhighsco/next-plugin-robots": "1.1.1",
    "@newhighsco/next-plugin-sitemap": "1.0.7",
    "@newhighsco/next-plugin-svgr": "1.0.2",
    "babel-loader": "8.1.0",
    "jdown": "1.0.4",
    "next": "9.4.4",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.2.0",
    "next-ga": "2.3.4",
    "next-images": "1.4.0",
    "next-seo": "4.7.1",
    "next-transpile-modules": "3.3.0",
    "next-videos": "1.1.4",
    "param-case": "3.0.3",
    "pluralize": "8.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "1.26.9",
    "sourcebit": "0.8.1",
    "sourcebit-target-next": "0.5.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@newhighsco/browserslist-config": "1.0.2",
    "@newhighsco/commitlint-config": "1.0.1",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.4",
    "@newhighsco/prettier-config": "2.0.3",
    "@newhighsco/release-config": "1.0.26",
    "@newhighsco/renovate-config": "1.4.0",
    "@newhighsco/stylelint-config": "2.0.18",
    "eslint": "7.3.1",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "stylelint": "13.6.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "staging"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
