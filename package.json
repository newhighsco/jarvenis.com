{
  "name": "@newhighsco/jarvenis.com",
  "description": "Jarvenis website",
  "version": "1.19.240",
  "author": "New High Score",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/jarvenis.com"
  },
  "homepage": "https://jarvenis.com",
  "bugs": {
    "url": "https://github.com/newhighsco/jarvenis.com/issues"
  },
  "files": [
    "public",
    "src",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf {.next,public/{robots.txt,{atom,rss,sitemap*}.xml},.sourcebit-*cache.json}",
    "start": "next dev",
    "build": "next build",
    "postbuild": "concurrently yarn:postbuild:*",
    "postbuild:sitemap": "next-sitemap",
    "postbuild:rss": "next-rss",
    "test": "yarn lint",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "lint:css": "stylelint --cache --ignore-path .gitignore '**/*.?(s)css'",
    "format": "concurrently yarn:format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.16.3",
    "@newhighsco/next-plugin-svgr": "3.0.130",
    "@newhighsco/press-start": "2.2.15",
    "concurrently": "8.2.2",
    "marked": "13.0.2",
    "next": "14.2.4",
    "next-compose-plugins": "2.2.1",
    "next-mdx-frontmatter": "0.0.4",
    "next-mdx-remote": "5.0.0",
    "next-rss": "1.0.1",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.39",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "1.77.6",
    "sourcebit": "0.11.1",
    "sourcebit-source-filesystem": "0.2.0",
    "sourcebit-target-next": "0.8.3",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@newhighsco/browserslist-config": "1.1.0",
    "@newhighsco/commitlint-config": "1.1.40",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.192",
    "@newhighsco/postcss-config": "3.5.253",
    "@newhighsco/prettier-config": "2.1.25",
    "@newhighsco/release-config": "1.3.77",
    "@newhighsco/stylelint-config": "3.1.85",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "prettier": "3.3.2",
    "semantic-release": "24.0.0",
    "stylelint": "16.6.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "local>newhighsco/.github:renovate-config"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
