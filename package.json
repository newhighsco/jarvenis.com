{
  "name": "@newhighsco/jarvenis.com",
  "description": "Jarvenis website",
  "version": "1.1.4",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "config": {
    "title": "Jarvenis"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/jarvenis.com"
  },
  "homepage": "https://jarvenis.com",
  "bugs": {
    "url": "https://github.com/newhighsco/jarvenis.com/issues"
  },
  "scripts": {
    "start": "next dev",
    "start:storybook": "start-storybook --port 9001 --docs --ci --quiet",
    "build": "npm-run-all build:next",
    "build:next": "next build && next export --outdir build",
    "build:storybook": "build-storybook --output-dir build/storybook --docs --quiet",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:css": "npm run lint:css -- --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "2.8.0",
    "@newhighsco/next-plugin-robots": "1.1.0",
    "@newhighsco/next-plugin-sitemap": "1.0.2",
    "@newhighsco/next-plugin-svgr": "1.0.0",
    "babel-loader": "8.1.0",
    "next": "9.3.5",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.1.0",
    "next-ga": "2.3.4",
    "next-images": "1.4.0",
    "next-seo": "4.4.0",
    "next-transpile-modules": "3.2.0",
    "next-videos": "1.1.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "1.26.5"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@newhighsco/browserslist-config": "1.0.1",
    "@newhighsco/commitlint-config": "1.0.1",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.1.1",
    "@newhighsco/prettier-config": "2.0.2",
    "@newhighsco/release-config": "1.0.20",
    "@newhighsco/renovate-config": "1.3.1",
    "@newhighsco/storybook-preset": "2.3.1",
    "@newhighsco/stylelint-config": "2.0.9",
    "@storybook/react": "5.3.18",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.0.7",
    "stylelint": "13.3.3"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
