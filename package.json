{
  "name": "@newhighsco/jarvenis.com",
  "description": "Jarvenis website",
  "version": "1.23.59",
  "author": "New High Score",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/jarvenis.com"
  },
  "homepage": "https://jarvenis.com",
  "bugs": {
    "url": "https://github.com/newhighsco/jarvenis.com/issues"
  },
  "files": [
    "public",
    "src",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf {.next,public/{robots.txt,{atom,rss,sitemap*}.xml},.sourcebit-*cache.json}",
    "start": "next dev --webpack",
    "build": "next build --webpack",
    "postbuild": "concurrently yarn:postbuild:*",
    "postbuild:sitemap": "next-sitemap",
    "test": "yarn lint",
    "analyze": "omlet analyze",
    "lint": "concurrently yarn:lint:*",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint --cache --ignore-path .gitignore '**/*.?(s)css'",
    "format": "concurrently yarn:format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.25.5",
    "@newhighsco/next-plugin-svgr": "3.0.157",
    "@newhighsco/press-start": "2.6.24",
    "@omlet/cli": "2.0.0",
    "concurrently": "9.2.1",
    "marked": "17.0.1",
    "next": "16.1.2",
    "next-compose-plugins": "2.2.1",
    "next-mdx-frontmatter": "0.0.4",
    "next-mdx-remote": "5.0.0",
    "next-rss": "1.0.1",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sass": "1.97.2",
    "sourcebit": "0.11.1",
    "sourcebit-source-filesystem": "0.2.0",
    "sourcebit-target-next": "0.8.3",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@newhighsco/browserslist-config": "2.0.0",
    "@newhighsco/commitlint-config": "1.1.46",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "6.0.7",
    "@newhighsco/postcss-config": "4.0.10",
    "@newhighsco/prettier-config": "2.3.2",
    "@newhighsco/release-config": "1.4.20",
    "@newhighsco/stylelint-config": "4.0.19",
    "@newhighsco/svgr-config": "1.0.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "prettier": "3.8.0",
    "semantic-release": "25.0.2",
    "stylelint": "16.26.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
