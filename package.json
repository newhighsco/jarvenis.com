{
  "name": "@newhighsco/jarvenis.com",
  "description": "Jarvenis website",
  "version": "1.15.31",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/jarvenis.com"
  },
  "homepage": "https://jarvenis.com",
  "bugs": {
    "url": "https://github.com/newhighsco/jarvenis.com/issues"
  },
  "files": [
    "public",
    "src",
    "next-sitemap.js",
    "next.config.js",
    "postcss.config.js",
    "site.config.js"
  ],
  "scripts": {
    "clean": "rm -rf {.next,public/{downloads,robots.txt,{atom,rss,sitemap*}.xml},.sourcebit-*cache.json}",
    "start": "next dev",
    "build": "next build",
    "postbuild": "npm-run-all --parallel postbuild:*",
    "postbuild:sitemap": "next-sitemap",
    "postbuild:rss": "next-rss",
    "test": "yarn lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.json,.jsx,.mdx .",
    "lint:css": "stylelint '**/*.{css,scss}' --cache --ignore-path .gitignore",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:css": "yarn lint:css --fix"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.13.7",
    "@newhighsco/next-plugin-svgr": "3.0.45",
    "babel-loader": "8.2.3",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "marked": "4.0.12",
    "next": "12.1.1",
    "next-compose-plugins": "2.2.1",
    "next-mdx-frontmatter": "0.0.4",
    "next-mdx-remote": "4.0.0",
    "next-optimized-images": "2.6.2",
    "next-rss": "1.0.1",
    "next-seo": "5.1.0",
    "next-sitemap": "2.5.13",
    "next-transpile-modules": "9.0.0",
    "next-videos": "1.4.0",
    "postcss": "8.4.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "responsive-loader": "2.3.0",
    "sass": "1.49.9",
    "sharp": "0.30.3",
    "sourcebit": "0.11.1",
    "sourcebit-source-filesystem": "0.2.0",
    "sourcebit-target-next": "0.8.1",
    "sourcebit-transform-assets": "0.3.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@newhighsco/browserslist-config": "1.0.3",
    "@newhighsco/commitlint-config": "1.0.24",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "3.1.21",
    "@newhighsco/postcss-config": "3.4.73",
    "@newhighsco/prettier-config": "2.0.18",
    "@newhighsco/release-config": "1.1.6",
    "@newhighsco/stylelint-config": "3.0.23",
    "eslint": "8.11.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.1",
    "semantic-release": "19.0.2",
    "stylelint": "14.6.1"
  },
  "browserslist": [
    "extends @newhighsco/browserslist-config"
  ],
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  },
  "stylelint": {
    "extends": [
      "@newhighsco/stylelint-config"
    ]
  }
}
